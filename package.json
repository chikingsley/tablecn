    "@testing-library/dom": "^10.4.1",
    "@testing-library/react": "^16.3.2",
    "@total-typescript/ts-reset": "^0.6.1",
    "@types/bun": "^1.3.9",
    "@types/node": "25.0.8",
    "@types/react": "19.2.8",
    "@types/react-dom": "^19.2.3",
