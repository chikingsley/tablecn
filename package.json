{
  "name": "tablecn",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "scripts": {
    "clean": "rimraf --glob **/node_modules **/dist bun.lock **/.tsbuildinfo",
    "build": "bun build src/server/index.ts --outdir dist/app --target bun --minify",
    "dev": "bun run dev:docker",
    "dev:prodlike": "bun run build && cd dist/app && NODE_ENV=production bun --env-file=../../.env index.js",
    "test": "bun test",
    "dev:docker": "bun run db:start && bun run dev:local",
    "dev:local": "bun --hot src/server/index.ts",
    "start": "bun src/server/index.ts",
    "ollie": "bun install && bun run db:start && bun run db:setup",
    "lint": "biome check .",
    "lint:fix": "biome check . --write",
    "typecheck": "tsc --noEmit",
    "check": "ultracite check",
    "shadcn": "bunx shadcn@latest",
    "build:registry": "shadcn build",
    "db:start": "docker compose up -d",
    "db:stop": "docker compose down",
    "db:migrate": "bun src/db/migrate.ts",
    "db:seed": "bun src/db/seed.ts",
    "db:setup": "bun run db:migrate && bun run db:seed",
    "db:reset": "bun run db:stop && rimraf docker-data && bun run db:start && sleep 10 && bun run db:setup",
    "build:server": "bun run typecheck",
    "fix": "ultracite fix"
  },
  "dependencies": {
    "@dnd-kit/core": "^6.3.1",
    "@dnd-kit/modifiers": "^9.0.0",
    "@dnd-kit/sortable": "^10.0.0",
    "@dnd-kit/utilities": "^3.2.2",
    "@hookform/resolvers": "^5.2.2",
    "@radix-ui/react-avatar": "^1.1.11",
    "@radix-ui/react-checkbox": "^1.3.3",
    "@radix-ui/react-dialog": "^1.1.15",
    "@radix-ui/react-direction": "^1.1.1",
    "@radix-ui/react-dropdown-menu": "^2.1.16",
    "@radix-ui/react-label": "^2.1.8",
    "@radix-ui/react-popover": "^1.1.15",
    "@radix-ui/react-portal": "^1.1.10",
    "@radix-ui/react-scroll-area": "^1.2.10",
    "@radix-ui/react-select": "^2.2.6",
    "@radix-ui/react-separator": "^1.1.8",
    "@radix-ui/react-slider": "^1.3.6",
    "@radix-ui/react-slot": "^1.2.4",
    "@radix-ui/react-switch": "^1.2.6",
    "@radix-ui/react-tabs": "^1.1.13",
    "@radix-ui/react-toggle": "^1.1.10",
    "@radix-ui/react-toggle-group": "^1.1.11",
    "@radix-ui/react-tooltip": "^1.2.8",
    "@tanstack/query-db-collection": "^1.0.23",
    "@tanstack/react-db": "^0.1.70",
    "@tanstack/react-query": "^5.90.21",
    "@tanstack/react-table": "^8.21.3",
    "@tanstack/react-virtual": "^3.13.18",
    "bun-plugin-tailwind": "^0.1.2",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "cmdk": "^1.1.1",
    "date-fns": "^4.1.0",
    "export-to-csv": "^1.4.0",
    "geist": "^1.7.0",
    "lucide-react": "^0.562.0",
    "motion": "^12.34.0",
    "nanoid": "^5.1.6",
    "next-themes": "^0.4.6",
    "nuqs": "^2.8.8",
    "postgres": "^3.4.8",
    "react": "19.2.3",
    "react-day-picker": "9.13.0",
    "react-dom": "19.2.3",
    "react-hook-form": "^7.71.1",
    "react-resizable-panels": "^4.6.2",
    "react-router-dom": "^7.1.5",
    "shadcn": "3.6.3",
    "sonner": "^2.0.7",
    "tailwind-merge": "^3.4.0",
    "vaul": "^1.1.2",
    "zod": "^4.3.6"
  },
  "devDependencies": {
    "@biomejs/biome": "2.3.13",
